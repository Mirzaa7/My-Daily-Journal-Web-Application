<%- include("partials/header") -%>

    <h1 class="newPostTitle">Create a new journal post</h1>
    <form action="/compose" method="post">
        <label>Title</label>
        <div>
            <input type="text" name="titleInput" id="textBox" autocomplete="off" >
        </div>
        <label>Post Text</label>
        <div>
            <textarea name="postInput" id="textBox" cols="30" rows="10" autocomplete="off"></textarea>
        </div>
        <button type="submit" class="btn btn-primary">Publish</button>
    </form>

